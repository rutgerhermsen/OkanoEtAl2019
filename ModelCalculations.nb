9fca118dca1f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"solveSystemP", "[", 
   RowBox[{
   "jL_", ",", " ", "j05A_", ",", " ", "kKA_", ",", " ", "kDA_", ",", " ", 
    "nKA_", ",", "nDA_", ",", " ", "h0A_", ",", " ", "\[Nu]KA_", ",", " ", 
    "mA_", ",", "jGInit_", ",", " ", "sInit_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"sol", ",", " ", "sol2", ",", " ", "f"}], "}"}], ",", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"sol", " ", "=", 
       RowBox[{"FindMinimum", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"jK", "[", 
                RowBox[{
                "\[Nu]KA", ",", "jL", ",", " ", "j05A", ",", " ", "h0A", ",", 
                 " ", "s", ",", " ", "kKA", ",", " ", "nKA", ",", 
                 RowBox[{"\[Lambda]F", "[", 
                  RowBox[{
                  "a", ",", " ", "b", ",", " ", "jG", ",", " ", "jL"}], "]"}],
                  ",", " ", "\[Lambda]C", ",", " ", "mA"}], "]"}], " ", "-", 
               RowBox[{"jD", "[", 
                RowBox[{
                "\[Alpha]D", ",", "s", ",", " ", "kDA", ",", " ", "nDA", ",", 
                 RowBox[{"\[Lambda]F", "[", 
                  RowBox[{
                  "a", ",", " ", "b", ",", " ", "jG", ",", " ", "jL"}], "]"}],
                  ",", " ", "\[CapitalDelta]v", ",", " ", "sv", ",", " ", 
                 "sh", ",", " ", "\[CapitalDelta]h"}], "]"}]}], ")"}], "^", 
             "2"}], " ", "+", " ", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"jK", "[", 
                RowBox[{
                "\[Nu]KA", ",", "jL", ",", " ", "j05A", ",", " ", "h0A", ",", 
                 " ", "s", ",", " ", "kKA", ",", " ", "nKA", ",", 
                 RowBox[{"\[Lambda]F", "[", 
                  RowBox[{
                  "a", ",", " ", "b", ",", " ", "jG", ",", " ", "jL"}], "]"}],
                  ",", " ", "\[Lambda]C", ",", " ", "mA"}], "]"}], "-", 
               "jG"}], ")"}], "^", "2"}]}], ",", "\[IndentingNewLine]", 
           RowBox[{"s", ">", "0"}], ",", 
           RowBox[{"jG", ">", "0"}], ",", " ", 
           RowBox[{"jG", "<", "30"}]}], "\[IndentingNewLine]", "}"}], ",", 
         " ", "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"jG", ",", "jGInit"}], "}"}], ",", " ", 
           RowBox[{"{", 
            RowBox[{"s", ",", " ", "sInit"}], "}"}]}], "}"}], ",", " ", 
         "\[IndentingNewLine]", 
         RowBox[{"Method", "\[Rule]", "\"\<Automatic\>\""}], ",", " ", 
         "\[IndentingNewLine]", 
         RowBox[{"WorkingPrecision", "\[Rule]", "50"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"PrecisionGoal", "\[Rule]", "10"}], ",", " ", 
         "\[IndentingNewLine]", 
         RowBox[{"AccuracyGoal", "\[Rule]", "10"}]}], "\[IndentingNewLine]", 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"sol2", " ", "=", " ", 
       RowBox[{"sol", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"f", " ", "=", " ", 
       RowBox[{"sol", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"f", ">", " ", 
         RowBox[{"10", "^", 
          RowBox[{"-", "6"}]}]}], " ", ",", " ", 
        RowBox[{
        "Print", "[", "\"\<Warning! f does not vanish at the minimum.\>\"", 
         "]"}]}], "]"}], ";", " ", 
      RowBox[{"(*", " ", 
       RowBox[{
       "uncomment", " ", "this", " ", "line", " ", "to", " ", "print", " ", 
        "the", " ", "value", " ", "of", " ", "F"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"Append", "[", 
       RowBox[{"sol2", ",", 
        RowBox[{"\[Lambda]", "\[Rule]", "  ", 
         RowBox[{"\[Lambda]F", "[", 
          RowBox[{"a", ",", " ", "b", ",", " ", 
           RowBox[{"jG", "/.", "sol2"}], ",", " ", "jL"}], "]"}]}]}], "]"}]}],
      "\[IndentingNewLine]", ")"}]}], "]"}]}]], "Input",
